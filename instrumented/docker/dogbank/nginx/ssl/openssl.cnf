[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_req

[dn]
C = BR
ST = Sao Paulo
L = Sao Paulo
O = DogBank
OU = Security Team
CN = localhost

[v3_req]
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = *.localhost
DNS.3 = localhost
DNS.4 = *.localhost
DNS.5 = dogbank.local
DNS.6 = *.dogbank.local
DNS.7 = lab.dogbank.dog
DNS.8 = *.lab.dogbank.dog
IP.1 = 127.0.0.1
IP.2 = ::1
IP.3 = 0.0.0.0
