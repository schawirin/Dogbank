{
  "timestamp": {
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
    }
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  },
  "logger": {
    "$resolver": "logger",
    "field": "name"
  },
  "message": {
    "$resolver": "message",
    "stringified": true
  },
  "thread": {
    "$resolver": "thread",
    "field": "name"
  },
  "exception": {
    "$resolver": "exception",
    "field": "stackTrace",
    "stackTrace": {
      "stringified": true
    }
  },
  "dd.trace_id": {
    "$resolver": "mdc",
    "key": "dd.trace_id"
  },
  "dd.span_id": {
    "$resolver": "mdc",
    "key": "dd.span_id"
  },
  "service": "${env:DD_SERVICE:-transaction-service}",
  "version": "${env:DD_VERSION:-1.0.0}",
  "env": "${env:DD_ENV:-dogbank}",
  "evento": {
    "$resolver": "mdc",
    "key": "evento"
  },
  "chave_pix": {
    "$resolver": "mdc",
    "key": "chave_pix"
  },
  "valor": {
    "$resolver": "mdc",
    "key": "valor"
  },
  "valor_numerico": {
    "$resolver": "mdc",
    "key": "valor_numerico"
  },
  "tipo_chave": {
    "$resolver": "mdc",
    "key": "tipo_chave"
  },
  "faixa_valor": {
    "$resolver": "mdc",
    "key": "faixa_valor"
  },
  "faixa_duracao": {
    "$resolver": "mdc",
    "key": "faixa_duracao"
  },
  "status_transacao": {
    "$resolver": "mdc",
    "key": "status_transacao"
  },
  "transaction_id": {
    "$resolver": "mdc",
    "key": "transaction_id"
  },
  "duracao_ms": {
    "$resolver": "mdc",
    "key": "duracao_ms"
  },
  "conta_origem_id": {
    "$resolver": "mdc",
    "key": "conta_origem_id"
  },
  "remetente_id": {
    "$resolver": "mdc",
    "key": "remetente_id"
  },
  "remetente_banco": {
    "$resolver": "mdc",
    "key": "remetente_banco"
  },
  "saldo_antes": {
    "$resolver": "mdc",
    "key": "saldo_antes"
  },
  "saldo_depois": {
    "$resolver": "mdc",
    "key": "saldo_depois"
  },
  "destinatario_id": {
    "$resolver": "mdc",
    "key": "destinatario_id"
  },
  "destinatario_nome": {
    "$resolver": "mdc",
    "key": "destinatario_nome"
  },
  "destinatario_cpf": {
    "$resolver": "mdc",
    "key": "destinatario_cpf"
  },
  "destinatario_banco": {
    "$resolver": "mdc",
    "key": "destinatario_banco"
