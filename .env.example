# Exemplo de .env.example para Datadog
# Copie este arquivo para .env e preencha com seus valores

# ============================================
# Datadog Configuration
# ============================================
DATADOG_API_KEY=seu_api_key_aqui
DATADOG_APP_KEY=sua_app_key_aqui
DATADOG_ENABLED=true

# ============================================
# Frontend
# ============================================
REACT_APP_API_URL=http://localhost:8080

# ============================================
# Database
# ============================================
DB_HOST=postgres
DB_PORT=5432
DB_NAME=dogbank
DB_USER=dogbank_user
DB_PASSWORD=seu_senha_segura

# ============================================
# Security
# ============================================
JWT_SECRET=sua_chave_secreta_jwt
